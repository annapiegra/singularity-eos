{
    "savename"  : "../../data/materials_64.sp5",
    "materials" : [
	{ // only matid is required. All others override defaults.
    // the most sensitive parameters relate to sie.
	    "matid"  : 5031,
	    "name"   : "air",
	    "rhomin" : 1e-2, // g/cm^3
	    "rhomax" : 10,
	    "numrho" : 64,
	    // "Tmin"   : 252, // kelvin
	    // "Tmax"   : 1e4,
	    "numT"   : 64,
	    "siemin" : 1e9, // erg/g
	    "siemax" : 1e12, // erg/g
	    "numsie" : 64
	},
	{
	    "matid" : 4272, // steel
	    "rhomin" : 1e-2, // g/cm^3
	    "rhomax" : 1e2, // g/cm^3
	    "numrho" : 64,
	    // "Tmin"   : 64, // kelvin
	    // "Tmax"   : 1e9,
	    "numT"   : 64,
	    "siemin" : 5e10,
	    "siemax" : 1e14,
      "numsie" : 64
	}
    ]
}
